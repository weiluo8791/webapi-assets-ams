{
    "id": "customerText",
    "version": 1,
    "resource": "v1/resource/resource/_version/1/",
    "status": "draft",
    "uri": "v1/resource/customerText/_version/1/",
    "cache": {
        "timeToLive": "short",
        "timeConsideredFresh": "always"
    },
    "properties": {
        "task": {
            "type": "string",
            "required": true,
            "responseOnly": true
        },
        "text": {
            "type": "string",
            "required": true,
            "responseOnly": true
        },
        "count": {
            "type": "string",
            "#doc": "Text Entry count"
        },
        "start": {
            "type": "string",
            "#doc": "Start range"
        },
        "end": {
            "type": "string",
            "#doc": "End range"
        },
        "events": {
            "type": "object[]",
            "object": {
                "properties": {
                    "event": {
                        "type": "string"
                    },
                    "lines": {
                        "type": "object[]",
                        "object": {
                            "properties": {
                                "number": {
                                    "type": "string"
                                },
                                "text": {
                                    "type": "string",
                                    "#doc": "Up to 80 characters, Will be nil for blank lines."
                                }
                            }
                        }
                    }
                }
            }
        },
        "warnings": {
            "type": "object[]",
            "object": {
                "properties": {
                    "warning.code": {
                        "type": "string"
                    },
                    "wac.from": {
                        "type": "string"
                    },
                    "wac.to": {
                        "type": "string"
                    }
                }
            }
        }
    }
}